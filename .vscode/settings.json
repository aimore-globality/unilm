{
    "jupyter.jupyterServerType": "local",
    "python.testing.pytestArgs": [
        "markuplm"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "jupyter.sendSelectionToInteractiveWindow": true,
    "key": "shift+enter",
    "command": "jupyter.execSelectionInteractive",
    "when": "editorTextFocus",
    "workbench.colorCustomizations": {
        "editorGroup.border": "#f5f0b0",
        "sash.hoverBorder": "#f5f0b0",
        "titleBar.activeBackground": "#f0e883",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#f0e88399",
        "titleBar.inactiveForeground": "#15202b99",
        "commandCenter.border": "#15202b99"
    },
    "vs-external-tools.externalCommand1.command": "/home/ubuntu/miniconda3/bin/globality-black",
    "vs-external-tools.externalCommand1.args": ["$(ItemPath)"],
    "vs-external-tools.externalCommand2.command": "/Users/aimoredutra/miniconda3/bin/isort",
    "vs-external-tools.externalCommand2.args": ["$(ItemPath)"],
    "jupyter.experiments.optInto": [
        "CustomEditor"
    ],
    "debug.allowBreakpointsEverywhere": true,
    "jupyter.debugJustMyCode": false,
}