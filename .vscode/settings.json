{
    "jupyter.jupyterServerType": "local",
    "python.testing.pytestArgs": [
        "markuplm"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "jupyter.sendSelectionToInteractiveWindow": true,
    "key": "shift+enter",           "command": "jupyter.execSelectionInteractive", "when": "editorTextFocus",
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#c26060c0",
        "editor.selectionHighlightBackground": "#00ff553a",
        "editor.findMatchBackground": "#00cc44f8",
        "editor.findMatchHighlightBackground": "#ff7b00d0"
    }
}